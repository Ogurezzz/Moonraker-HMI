{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [{
		"name": "(gdb) Запустить",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/bin/Moonraker-HMI",
		"args": [
			//"'http://192.168.5.10:7125/printer/objects/query?extruder=temperature,target&heater_bed=temperature,target&fan=speed&gcode_move=position,speed_factor&virtual_sdcard=progress&print_stats=print_duration&filament_switch_sensor%20Runout_Sensor=filament_detected'",
			"http://192.168.5.10:7125",
			"/dev/ttyUSB0"],
		"stopAtEntry": false,
		"cwd": "${fileDirname}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
			{
				"description": "Включить автоматическое форматирование для gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			},
			{
				"description": "Задать для варианта приложения дизассемблирования значение Intel",
				"text": "-gdb-set disassembly-flavor intel",
				"ignoreFailures": true
			}
		]
	}]
}